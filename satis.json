{
    "name": "My Private Composer Repository",
    "homepage": "https://github.com/jinridaren2024/composer-packages/",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jinridaren2024/money-calc.git"
        }
    ],
    "require-all": true,
    "output-dir": "public",
    "archive": {
        "directory": "dist",
        "format": "zip",
        "prefix-url": "https://github.com/jinridaren2024/composer-packages/",
        "skip-dev": false
    },
   
    "config": {
        "github-oauth": {
            "github.com": "GITHUB_TOKEN_PLACEHOLDER"
        }
    }
}
